<h1>@ViewBag.title</h1>


<form method="post">
    @foreach(Cheese cheese in ViewBag.cheeses)
    {
        <input name="cheeseIds" type="checkbox" value="@cheese.CheeseId" /> 
        <label for="@cheese.CheeseId">@cheese.Name</label>
        <br />
    }
    <input type="submit" value="Remove Cheese" />
</form>